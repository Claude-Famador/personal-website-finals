{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)\\.(?:jpg|jpeg|gif|png|mp4|webm|woff2)$",
      "destination": "/$1"
    }
  ]
}